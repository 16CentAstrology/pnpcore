{
  "PnPCore": {
    "Credentials": {
      "CredentialManagerAuthentication": {
        "CredentialManagerName": "{credential_manager_item_name}"
      }
    },
    "Sites": {
      "DemoSite": {
        "SiteUrl": "{modern_team_site_url}",
        "AuthenticationProviderName": "CredentialManagerAuthentication"
      }
    }
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "{tenant_name}",
    "TenantId": "{tenant_id}",
    "ClientId": "{app_id}",
    "CallbackPath": "/signin-oidc"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
