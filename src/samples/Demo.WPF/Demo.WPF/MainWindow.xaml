<Window x:Class="Demo.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demo.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnSite" Content="Site Information" HorizontalAlignment="Left" Height="34" Margin="50,25,0,0" VerticalAlignment="Top" Width="115" Click="btnSite_Click" />
        <Button x:Name="btnList" Content="List Information" HorizontalAlignment="Left" Height="34" Margin="220,25,0,0" VerticalAlignment="Top" Width="114" Click="btnList_Click"/>
        <Button x:Name="btnTeam" Content="Teams Information" HorizontalAlignment="Left" Height="34" Margin="382,25,0,0" VerticalAlignment="Top" Width="115" Click="btnTeam_Click"/>
        <TextBlock x:Name="TxtResults" HorizontalAlignment="Left" Height="273" Margin="50,95,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="447" IsEnabled="False"/>

    </Grid>
</Window>
