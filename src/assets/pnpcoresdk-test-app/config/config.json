{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "pn-p-core-sdk-banner-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/pnPCoreSdkBanner/PnPCoreSdkBannerWebPart.js",
          "manifest": "./src/webparts/pnPCoreSdkBanner/PnPCoreSdkBannerWebPart.manifest.json"
        }
      ]
    },
    "pn-p-core-sdk-header-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/pnPCoreSdkHeader/PnPCoreSdkHeaderApplicationCustomizer.js",
          "manifest": "./src/extensions/pnPCoreSdkHeader/PnPCoreSdkHeaderApplicationCustomizer.manifest.json"
        }
      ]
    },
    "pn-p-core-sdk-field-customizer-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/pnPCoreSdkFieldCustomizer/PnPCoreSdkFieldCustomizerFieldCustomizer.js",
          "manifest": "./src/extensions/pnPCoreSdkFieldCustomizer/PnPCoreSdkFieldCustomizerFieldCustomizer.manifest.json"
        }
      ]
    },
    "pn-p-core-sdk-test-command-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/pnPCoreSdkTestCommand/PnPCoreSdkTestCommandCommandSet.js",
          "manifest": "./src/extensions/pnPCoreSdkTestCommand/PnPCoreSdkTestCommandCommandSet.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PnPCoreSdkBannerWebPartStrings": "lib/webparts/pnPCoreSdkBanner/loc/{locale}.js",
    "PnPCoreSdkHeaderApplicationCustomizerStrings": "lib/extensions/pnPCoreSdkHeader/loc/{locale}.js",
    "PnPCoreSdkFieldCustomizerFieldCustomizerStrings": "lib/extensions/pnPCoreSdkFieldCustomizer/loc/{locale}.js",
    "PnPCoreSdkTestCommandCommandSetStrings": "lib/extensions/pnPCoreSdkTestCommand/loc/{locale}.js"
  }
}
